{
  "name": "@aptos-labs/create-docs",
  "version": "0.0.3",
  "description": "A CLI tool for creating Aptos Nextra Docs",
  "type": "module",
  "files": [
    "!tests/**/*",
    "dist/**/*",
    "!dist/**/*.js.map",
    "bin/**/*"
  ],
  "bin": {
    "create-aptos-docs": "./dist/index.cjs"
  },
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "prebuild": "run-s clean",
    "predev": "run-s clean",
    "clean": "rm -rf dist",
    "typecheck": "tsc --noEmit",
    "_dev": "ncc build ./index.ts -w -o dist/",
    "prerelease": "node ../../scripts/rm.mjs dist",
    "release": "ncc build ./index.ts -o ./dist/ --minify --no-cache --no-source-map-register",
    "_build": "pnpm release",
    "lint-fix": "pnpm prettier -w --plugin prettier-plugin-tailwindcss 'templates/*-tw/{ts,js}/{app,pages}/**/*.{js,ts,tsx}'"
  },
  "keywords": [
    "docs",
    "nextra",
    "aptos"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/aptos-labs/nextra"
  },
  "engines": {
    "node": ">=12.0"
  },
  "bugs": {
    "url": "https://github.com/aptos-labs/nextra/issues"
  },
  "homepage": "https://github.com/aptos-labs/nextra#readme",
  "dependencies": {
    "@aws-sdk/shared-ini-file-loader": "^3.374.0",
    "async-sema": "^3.1.1",
    "aws-sdk": "^2.1559.0",
    "chalk": "^5.2.0",
    "commander": "^10.0.0",
    "dayjs": "^1.11.7",
    "dotenv": "^16.0.3",
    "node-fetch": "^3.3.0",
    "ora": "^6.1.2"
  },
  "devDependencies": {
    "@types/async-retry": "1.4.2",
    "@types/ci-info": "2.0.0",
    "@types/cross-spawn": "6.0.0",
    "@types/node": "^20.2.5",
    "@types/prompts": "2.4.2",
    "@types/tar": "6.1.5",
    "@types/validate-npm-package-name": "3.0.0",
    "@vercel/ncc": "0.34.0",
    "async-retry": "1.3.1",
    "ci-info": "watson/ci-info#f43f6a1cefff47fb361c88cf4b943fdbcaafe540",
    "commander": "2.20.0",
    "conf": "10.2.0",
    "cross-spawn": "7.0.3",
    "fast-glob": "3.3.1",
    "npm-run-all": "^4.1.5",
    "picocolors": "1.0.0",
    "prettier-plugin-tailwindcss": "0.3.0",
    "prompts": "2.4.2",
    "tar": "6.1.15",
    "ts-node": "^10.9.1",
    "tsup": "^6.5.0",
    "typescript": "^4.9.4",
    "update-check": "1.5.4",
    "validate-npm-package-name": "3.0.0"
  }
}
